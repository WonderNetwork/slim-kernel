{
    "name": "wondernetwork/slim-kernel",
    "type": "library",
    "require": {
        "php": ">=8.3",
        "ext-json": "*",
        "slim/slim": "^4.14",
        "slim/psr7": "^1.6",
        "php-di/php-di": "^7",
        "php-di/slim-bridge": "^3.4",
        "symfony/console": "^5.4 | ^6 | ^7",
        "symfony/finder": "^5.4 | ^6 | ^7",
        "symfony/property-access": "^7.2",
        "symfony/serializer": "^7.2",
        "phpstan/phpdoc-parser": "^2.0",
        "phpdocumentor/reflection-docblock": "^5.6",
        "wondernetwork/php-collection-library": "^10.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WonderNetwork\\SlimKernel\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WonderNetwork\\SlimKernel\\": "tests/",
            "Acme\\": [
                "tests/Resources/App/src/",
                "tests/Resources/ErrorMiddleware/src/"
            ]
        }
    },
    "authors": [
        {
            "name": "Maciej ≈Åebkowski",
            "email": "m.lebkowski@gmail.com"
        }
    ],
    "require-dev": {
        "phpstan/phpstan": "^2",
        "phpunit/phpunit": "^11",
        "bnf/phpstan-psr-container": "^1.0"
    }
}
